                </div>

            </div>
        </div>
        
        <div class="col-md-8">
            <%- include vstg_varinfo.ejs%>
            <%- include vstg_varadd.ejs %>
            <%- include vstg_varadd2.ejs %>
            <%- include vstg_varedit.ejs %>
            <%- include vstg_varedit2.ejs %>
            <%- include vstg_vardelete.ejs %>
            <%- include vstg_approve.ejs %>
            <%- include vstg_varapprove.ejs %>
            <%- include vstg_main.ejs%>
        </div>
    </div>
    
</div>