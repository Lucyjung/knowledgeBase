<div id="vstg-vardelete-table" style="display: none;">
<table class="table" >
    <tbody>
        <tr>
            <td >
                Variable Name
            </td>
            <td colspan="5">
                <input type="text" class="form-control" id="vstg-vardelete-table-varname" readonly>
            </td>
        </tr>
        <tr>
            <td>
                Min Value
            </td>
            <td>
                <input type="text" class="form-control" id="vstg-vardelete-table-minval" name="minValue" readonly>
            </td>
            <td>
                Max Value
            </td>
            <td>
                <input type="text" class="form-control" id="vstg-vardelete-table-maxval" name="maxValue" readonly>
            </td>
            <td>
                E-Level
            </td>
            <td>
                <input type="text" class="form-control" id="vstg-vardelete-table-elevel" name="eLevel" readonly>
            </td>
        </tr>
        <tr>
            <td>
                Soft Version
            </td>
            <td>
                <input type="text" class="form-control" id="vstg-vardelete-table-softver" name="softVersion" readonly>
            </td>
            <td>
                DD Version
            </td>
            <td colspan="3">
                <input type="text" class="form-control" id="vstg-vardelete-table-ddver"  name="ddVersion" readonly>
            </td>
        </tr>
    </tbody>
</table>
<table class="table" id="vstg-vardelete-casetable">
    <thead>
        <tr class="success">
            <td>
                No.    
            </td>
            <td colspan="5">
                Description
            </td>
            <td colspan="1">
                Soft Version
            </td>
            <td>
                Last Update
            </td>
            <td colspan="1">
                Select
            </td>
        </tr>
    </thead>
    <tbody>
    </tbody>
</table>
<div>
    <a href="#delete-case-modal" data-toggle="modal"><button type="button" class="btn btn-primary">Delete</button></a>
    <a href="#delete-all-modal" data-toggle="modal"><button type="button" class="btn btn-primary">Delete All</button></a>
</div>
</div>
<div class="modal fade" id="delete-case-modal" role="dialog" aria-labelledby="DeleteCase_ModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    ×
                </button>
                <h4 class="modal-title" id="DeleteUser_ModalLabel">
                    Confirm to Delete 
                </h4>
            </div>
            <div class="modal-body">
                Do you want to delete this case?
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="vstg-vardelete-case-button">
                    Delete
                </button> 
                <button type="button" class="btn btn-default" data-dismiss="modal">
                    Close
                </button> 	
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="delete-all-modal" role="dialog" aria-labelledby="DeleteAll_ModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    ×
                </button>
                <h4 class="modal-title" id="DeleteUser_ModalLabel">
                    Confirm to Delete 
                </h4>
            </div>
            <div class="modal-body">
                Do you want to delete this variable?
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="vstg-vardelete-all-button">
                    Delete
                </button> 
                <button type="button" class="btn btn-default" data-dismiss="modal">
                    Close
                </button> 	
            </div>
        </div>
    </div>
</div>